{
  "name": "yahoo-pipes-backend",
  "version": "1.0.0",
  "description": "Backend API for Yahoo Pipes 2025",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "migrate": "node -r tsx/cjs src/scripts/migrate.ts",
    "validate-env": "tsx scripts/validate-env.ts",
    "generate-key": "node scripts/generate-encryption-key.js",
    "db:reset": "tsx src/scripts/reset-db.ts",
    "db:seed": "tsx src/scripts/seed-db.ts",
    "db:reset-seed": "tsx src/scripts/reset-and-seed.ts",
    "cache:clear": "tsx src/scripts/clear-cache.ts",
    "cleanup:unverified": "tsx src/scripts/cleanup-unverified-users.ts",
    "check-password": "tsx scripts/check-user-password.ts"
  },
  "keywords": [
    "yahoo-pipes",
    "data-mashup",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.6.12",
    "resend": "^6.5.2",
    "rss-parser": "^3.13.0",
    "validator": "^13.11.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.0",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.19.25",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/pg": "^8.10.9",
    "@types/validator": "^13.11.7",
    "fast-check": "^4.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.14"
  },
  "type": "commonjs"
}
